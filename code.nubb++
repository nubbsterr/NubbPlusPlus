# Nubb++ 1.3 Showcase

PRINT "Hello world," 
PRINT "Welcome to Nubb++ 1.3 ❤️"
PRINT ""


PRINT "Input a positive number >= 0: "
INPUT a

IF a == 1 THEN
PRINT "If/elif/else statements are now all fully supported!"
ENDIF

ELIF a == 1+1 THEN 
PRINT "We can compare identifiers with arithemtic expressions!"
ENDIF

ELSE
PRINT "We can else statements properly as well!"
ENDIF
